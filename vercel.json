{
  "version": 2,
  "installCommand": "corepack enable && corepack prepare pnpm@8.15.5 --activate && pnpm install",
  "buildCommand": "pnpm vitepress build docs",
  "outputDirectory": "docs/.vitepress/dist",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/pt",
      "permanent": true
    }
  ]
}
